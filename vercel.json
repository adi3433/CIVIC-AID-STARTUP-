{
    "version": 2,
    "builds": [
        {
            "src": "api/index.py",
            "use": "@vercel/python"
        },
        {
            "src": "web/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/index.py"
        },
        {
            "src": "/",
            "dest": "/web/landing.html"
        },
        {
            "src": "/(.*)",
            "dest": "/web/$1"
        }
    ]
}